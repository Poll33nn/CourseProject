<Window x:Class="Forestry.DefaultWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Forestry"
        mc:Ignorable="d"
        Title="Лесничество" Height="650" Width="800" ResizeMode="NoResize">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="100"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Border Background="#2E8B57" Margin="10" Padding="10 0 0 0" CornerRadius="2">
            <TextBlock 
                Text='ГКУ Архангельской области "Коношское лесничество"' 
                Foreground="White" 
                FontSize="16"
                VerticalAlignment="Center" />
        </Border>
        <Frame x:Name="DefaultFrame" NavigationUIVisibility="Hidden" Grid.Row="1"/>
    </Grid>
</Window>
